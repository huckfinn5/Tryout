<Set_OLine>:
    name: 'set_oline'
    name_text_input: o_name
    o_line_list: players_view
    canvas.before:
        Color:
            rgba: 1,1, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size
    BoxLayout:
        orientation: 'vertical'
        BoxLayout:
            size_hint_y: .15
            Label:
                id: oline_pos
                text: "O Line: Positions: " + str(root.positions) + "/7"
                font_name: "DejaVuSans"
                font_size: .35*self.size[1]
                color: 0,0,0,1
        BoxLayout:
            size_hint_y: .15
            Label:
                size_hint_x: 15
                color: 0,0,0,1
                text: "Name(s)"
                font_name: "DejaVuSans"
                font_size: .35*self.size[1]
            TextInput:
                size_hint_x: 30
                id: o_name
        BoxLayout:
            size_hint_y: .15
            Button:
                text: "Add"
                size_hint_x: 15
                font_name: "DejaVuSans"
                font_size: .39*self.size[1]
                on_press: root.add()
            Button:
                text: "Delete"
                size_hint_x: 15
                font_name: "DejaVuSans"
                font_size: .39*self.size[1]
                on_press: root.delete()

        ListView:
            id: players_view
            adapter:
                ListAdapter(data = [], cls = main.PlayerListButton)
        BoxLayout:
            size_hint_y: .15
            Button:
                text: "Home"
                font_name: "DejaVuSans"
                font_size: .75*self.size[1]
                outline_color: (1,1,.6)
                color: 1,1,.6,1
                outline_width: 0
                bold: False
                background_color: 0,0, 0, 1
                on_release:
                    root.revert()
                    app.root.current = 'home'
            Button:
                text: "Next"
                font_name: "DejaVuSans"
                font_size: .75*self.size[1]
                outline_color: (1,1,.6)
                color: 1,1,.6,1
                outline_width: 0
                bold: False
                background_color: 0,0, 0, 1
                on_release: app.root.current = 'set_dline' if root.length() == 7 else root.show()

