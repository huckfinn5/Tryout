<How_To_Use>:
    name: 'how_to_use'
    orientation: 'vertical'
    canvas.before:
        Color:
            rgba: 1,1, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size
    ScrollView:
        canvas.before:
            Color:
                rgba: 1,1, 1, 1
            Rectangle:
                size: self.size
                pos: self.pos
        scroll_y:1
        bar_margin: -15
        bar_width: 5
        bar_color: 0,0,0,1
        size_hint: .95,.88
        pos_hint: {'center_x': .5, 'center_y': .54}
        GridLayout:
            cols: 1
            size_hint_y: None
            height: self.minimum_height
            Label:
                color: 0,0,0,1
                text: root.text0
                font_name: "DejaVuSans"
                font_size: .0511*self.width
                text_size: self.width, None
                bold: 25
                size_hint_y: None
                height: self.texture_size[1]
            Label:
                color: 0,0,0,1
                text: root.text1
                font_name: "DejaVuSans"
                font_size: .0511*self.width
                text_size: self.width, None
                bold: 25
