<OpenIntro>:
    orientation: 'horizontal'
    canvas.before:
        Color:
            rgba: 1,1, .2, 1
        Rectangle:
            pos: self.pos
            size: self.size
    Button:
        font_name: "DejaVuSans"
        font_size: .32*self.size[0]
        size_hint: 1,1
        pos_hint: {'center_x': .5, 'center_y': .1}
        outline_color: (0,0,0)
        color: 0,0,0,1
        bold: True
        background_color: 0,0, 0, 0
        on_release: app.root.current = 'home'
    Image:
        id: picture
        source: root.image1
        size_hint: .9,.9
        pos_hint: {'center_x': .5, 'center_y': .5}

